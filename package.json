{
  "name": "cic",
  "version": "1.0.0",
  "engines": {
    "node": "6.10.1",
    "npm": "3.10.10"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:webui": "cd webui && npm install",
    "lint": "node_modules/.bin/eslint *.js server/app webui/src --ignore-pattern **/*.test.js",
    "test": "MONGODB_URI=mongodb://localhost:27017/cic_test PORT=7654 mocha --reporter spec server/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "dev:server": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon server/app/index.js",
    "dev:client": "cd webui && npm start",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "cd webui && npm run build",
    "prebuild": "npm run build:client",
    "build": "babel server --ignore server/app/webui -s -D -d build",
    "postbuild": "rimraf build/app/webui && ncp webui/build build/app/webui",
    "start": "node build/app",
    "start:prod": "NODE_ENV=production node build/app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forviz/cic.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/forviz/cic/issues"
  },
  "homepage": "https://github.com/forviz/cic#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "chalk": "^1.1.3",
    "cloudinary": "^1.9.0",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "cross-env": "^5.0.0",
    "cuid": "^1.3.8",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-flash": "0.0.2",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "jwks-rsa": "^1.1.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.10.2",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "styled-components": "^2.0.0"
  }
}
